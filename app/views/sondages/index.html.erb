<p id="notice"><%= notice %></p>

<h1>Sondages</h1>

<table>
  <thead>
    <tr>
      <th>Titre</th>
      <th>Description</th>
      <th>Slug</th>
      <th>Image</th>
      <th>Administrateur</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sondages.each do |sondage| %>
      <tr>
        <td><%= sondage.titre %></td>
        <td><%= sondage.description %></td>
        <td><%= sondage.slug %></td>
        <td><%= sondage.image %></td>
        <td><%= sondage.administrateur_id %></td>
        <td><%= link_to 'Show', sondage %></td>
        <td><%= link_to 'Edit', edit_sondage_path(sondage) %></td>
        <td><%= link_to 'Destroy', sondage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sondage', new_sondage_path %>
